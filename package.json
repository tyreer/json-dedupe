{
  "name": "json-dedupe",
  "version": "0.1.0",
  "description": "A command-line tool for deduplicating JSON records with comprehensive logging and validation",
  "main": "dist/dedupe/index.js",
  "bin": {
    "dedupe": "./dist/dedupe/cli.js"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --write src/ tests/",
    "build": "tsc",
    "dev": "ts-node src/dedupe/cli.ts",
    "start": "node dist/dedupe/cli.js"
  },
  "keywords": [
    "json",
    "deduplication",
    "cli",
    "data-processing",
    "logging"
  ],
  "author": "JSON Deduplication Tool Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.4.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
